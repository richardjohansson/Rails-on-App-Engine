<appengine-web-app xmlns='http://appengine.google.com/ns/1.0'>
  <application>richard-rails-test</application>
  <version>1</version>
  <public-root>/public</public-root>
  <static-files/>
  <resource-files>
    <exclude path='README'/>
    <exclude path='Rakefile'/>
    <exclude path='db/**'/>
    <exclude path='doc/**'/>
    <exclude path='log/**'/>
    <exclude path='script/**'/>
    <exclude path='test/**'/>
    <exclude path='tmp/**'/>
  </resource-files>
  <system-properties>
    <property name='os.arch' value=''/>
    <property name='jruby.management.enabled' value='false'/>
    <property name='jruby.rack.input.rewindable' value='false'/>
  </system-properties>
  <sessions-enabled>true</sessions-enabled>
</appengine-web-app>